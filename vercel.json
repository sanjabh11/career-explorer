{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "netlify/functions/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": "netlify/functions/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/\\$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "ONET_USERNAME": "ONET_USERNAME",
    "ONET_PASSWORD": "ONET_PASSWORD"
  }
}
